RewriteEngine On 

# for subdomains
RewriteCond %{HTTPS} !=on
RewriteCond %{HTTP_HOST} !^(www\.)?turnofspeed.in$ [NC]
RewriteCond %{HTTP_HOST} ^(?:www\.|)(.*)$ [NC]
#RewriteRule ^.*$ https://%1%{REQUEST_URI} [R,L]

# for main domains
RewriteCond %{HTTPS} !=on [OR]
RewriteCond %{HTTP_HOST} ^turnofspeed\.in$ [NC]
#RewriteRule ^.*$ https://www.turnofspeed.in%{REQUEST_URI} [R,L]

Options -Indexes

RewriteRule ^search_results/?$ search_results.php [L]
RewriteRule ^news.html/?$ news.php [L]
RewriteRule ^reviews.html/?$ reviews.php [L]
RewriteRule ^features.html/?$ features.php [L]
RewriteRule ^articles.html/?$ articles.php [L]
RewriteRule ^columns.html/?$ columns.php [L]
RewriteRule ^electric_vehicles.html/?$ electric_vehicles.php [L]
RewriteRule ^video_gallery.html/?$ video_gallery.php [L]
RewriteRule ^stories.html/?$ stories.php [L]
RewriteRule ^story/([^/\.]+)\.html/?$ view_story.php?id=$1 [L]
RewriteRule ^news/([^/\.]+)\.html/?$ view_news.php?n_id=$1 [L]
RewriteRule ^review/([^/\.]+)\.html/?$ view_review.php?p_id=$1 [L]
RewriteRule ^article/([^/\.]+)\.html/?$ view_article.php?b_id=$1 [L]
RewriteRule ^evs/([^/\.]+)\.html/?$ view_evs.php?ev_id=$1 [L]
RewriteRule ^column/([^/\.]+)\.html/?$ view_column.php?b_id=$1 [L]
RewriteRule ^feature/([^/\.]+)\.html/?$ view_feature.php?b_id=$1 [L]
RewriteRule ^video/([^/\.]+)\.html/?$ view_video.php?v_id=$1 [L]

RewriteRule ^rss/([^/\.]+)/?$ create_rss.php?rss=$1 [L]

ErrorDocument 404 /index.php

Order Deny,Allow
Deny from 218.20.7.38
Deny from 54.248.172.55

